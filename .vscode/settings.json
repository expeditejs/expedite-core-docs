{
    "markdown-pdf.convertOnSave": true,
    "markdown-pdf.convertOnSaveExclude": [
        "_^",
        "_navbar.md",
        "_sidebar.md",
        "_coverpage.md",
        "_404.md"
      ],
      "markdown-pdf.styles": [
        "/custom/markdown-pdf.css",
      ],
      "markdown-pdf.headerTemplate": "<div style=\"font-size: 9px; margin-left: auto; margin-right: 1cm; \"> <span class='date'></span></div>",
      "markdown-pdf.footerTemplate": "<div style=\"font-size: 9px; margin: 0 auto;\"> <span class='pageNumber'></span> / <span class='totalPages'></span></div>"
}